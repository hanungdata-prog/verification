{
  "name": "authgateway",
  "version": "1.0.0",
  "description": "Exotic Roleplay Gateway - Discord verification service",
  "main": "api/index.py",
  "scripts": {
    "dev": "npm run dev:python",
    "dev:python": "python run_server.py",
    "dev:uvicorn": "uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "build": "npm run build:static && npm run build:functions",
    "build:static": "node scripts/build-static.js",
    "build:functions": "node scripts/build-functions.js",
    "build:prepare": "node scripts/build.js",
    "start": "python run_server.py",
    "start:production": "uvicorn app.main:app --host 0.0.0.0 --port $PORT",
    "lint": "flake8 app/",
    "lint:fix": "autopep8 --in-place --recursive app/",
    "test": "python -m pytest tests/ -v",
    "install:python": "pip install -r requirements.txt",
    "setup": "npm run install:python && node scripts/setup.js",
    "vercel-build": "npm run build",
    "netlify-build": "npm run build"
  },
  "keywords": [
    "discord",
    "verification",
    "fastapi",
    "python",
    "auth",
    "gateway"
  ],
  "author": "Exotic Roleplay",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "chalk": "^4.1.2",
    "fs-extra": "^11.1.1",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/authgateway.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/authgateway/issues"
  },
  "homepage": "https://github.com/your-username/authgateway#readme"
}