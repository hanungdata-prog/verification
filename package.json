{
  "name": "authgateway",
  "version": "1.0.0",
  "description": "AuthGateway - Discord Verification Service",
  "main": "app/main.py",
  "scripts": {
    "build": "node scripts/build.js",
    "build:static": "node scripts/build-static.js",
    "build:functions": "node scripts/build-functions.js",
    "vercel-build": "npm run build:static && npm run build:functions",
    "setup": "node scripts/setup.js",
    "start": "python -m uvicorn app.main:app --host 0.0.0.0 --port 8000",
    "dev": "python -m uvicorn app.main:app --host 0.0.0.0 --port 8000 --reload"
  },
  "keywords": [
    "discord",
    "verification",
    "authgateway",
    "oauth2"
  ],
  "author": "AuthGateway Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/authgateway.git"
  }
}