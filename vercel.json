{
  "version": 2,
  "name": "authgateway",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/discord/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/verify(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/privacy",
      "dest": "/privacy.html"
    },
    {
      "src": "/discord-callback.html",
      "dest": "/discord-callback.html"
    },
    {
      "src": "/verify.html",
      "dest": "/verify.html"
    },
    {
      "src": "/verify-auto.html",
      "dest": "/verify-auto.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "PYTHONUNBUFFERED": "1",
    "BASE_URL": "https://apinode1a2b3c4d5e6f7g8h9i0j1k2l3m4n5o6p7q8r9s0t1u2-a64eej8it.vercel.app",
    "DISCORD_REDIRECT_URI": "https://apinode1a2b3c4d5e6f7g8h9i0j1k2l3m4n5o6p7q8r9s0t1u2-a64eej8it.vercel.app/discord/callback"
  }
}