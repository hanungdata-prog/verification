{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/verify",
      "dest": "api/index.py"
    },
    {
      "src": "/verify",
      "dest": "api/index.py"
    },
    {
      "src": "/discord/login",
      "dest": "api/index.py"
    },
    {
      "src": "/discord/callback",
      "dest": "api/index.py"
    },
    {
      "src": "/discord/callback(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/privacy",
      "dest": "privacy-policy.html"
    },
    {
      "src": "/admin/verifications",
      "dest": "api/index.py"
    },
    {
      "src": "/verify.html",
      "dest": "verify.html"
    },
    {
      "src": "/verify-auto.html",
      "dest": "verify-auto.html"
    },
    {
      "src": "/discord-callback.html",
      "dest": "discord-callback.html"
    },
    {
      "src": "/index.html",
      "dest": "index.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "favicon.png"
    },
    {
      "src": "/debug.js",
      "dest": "debug.js"
    },
    {
      "src": "/token-handler.js",
      "dest": "token-handler.js"
    },
    {
      "src": "/verification-check.js",
      "dest": "verification-check.js"
    },
    {
      "src": "/verification-bridge.js",
      "dest": "verification-bridge.js"
    },
    {
      "src": "/security.js",
      "dest": "security.js"
    },
    {
      "src": "/theme.js",
      "dest": "theme.js"
    },
    {
      "src": "/animations.js",
      "dest": "animations.js"
    },
    {
      "src": "/moon-ufo-loading.js",
      "dest": "moon-ufo-loading.js"
    },
    {
      "src": "/(.*\\.(css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "$1"
    },
    {
      "src": "/api/csrf-nonce",
      "dest": "api/index.py"
    },
    {
      "src": "/security-error.html",
      "dest": "api/index.py"
    },
    {
      "src": "/test-security-error",
      "dest": "api/index.py"
    },
    {
      "src": "/test-token",
      "dest": "api/index.py"
    },
    {
      "src": "/check-vpn/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/check-security",
      "dest": "api/index.py"
    },
    {
      "src": "/api/check-verification/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/api/check-user-verification",
      "dest": "api/index.py"
    },
    {
      "src": "/api/verification-stats",
      "dest": "api/index.py"
    },
    {
      "src": "/api/decode-token/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}