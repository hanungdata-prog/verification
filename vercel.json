{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/verify.html",
      "dest": "verify.html"
    },
    {
      "src": "/verify-auto.html",
      "dest": "verify-auto.html"
    },
    {
      "src": "/discord-callback.html",
      "dest": "discord-callback.html"
    },
    {
      "src": "/index.html",
      "dest": "index.html"
    },
    {
      "src": "/",
      "dest": "index.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "favicon.ico"
    },
    {
      "src": "/favicon.png",
      "dest": "favicon.png"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico))",
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}