{
  "version": 2,
  "name": "authgateway",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/discord/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/verify(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/privacy",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/verify.html",
      "dest": "/public/verify.html"
    },
    {
      "src": "/discord-callback.html",
      "dest": "/public/discord-callback.html"
    },
    {
      "src": "/verify-auto.html",
      "dest": "/public/verify-auto.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "PYTHONUNBUFFERED": "1"
  }
}